@model string


<a asp-action="Details" asp-route-id="@Model" class="btn btn-info text-white">
    <i class="bi bi-eye"></i> Details
</a>
                   
<td>
    <a asp-action="Edit" asp-route-id="@Model" class="btn btn-primary text-white">
        <i class="bi bi-pencil-square"></i> Update
    </a>
</td>
<td>
    <a asp-action="Delete" asp-route-id="@Model" class="btn btn-danger text-white">
        <i class="bi bi-trash"></i> Delete
    </a>
</td>